{
    "$schema": "https://joplinapp.org/schema/settings.json",
    "altInstanceId": "",
    "dateFormat": "YYYY-MM-DD",
    "editor.codeView": true,
    "editor.spellcheckBeta": true,
    "locale": "en_GB",
    "markdown.plugin.softbreaks": false,
    "markdown.plugin.typographer": false,
    "notes.perFieldReverse": {
        "order": false,
        "title": false,
        "user_created_time": true,
        "user_updated_time": true
    },
    "notes.sortOrder.field": "order",
    "notes.sortOrder.reverse": false,
    "noteVisiblePanes": [
        "editor",
        "viewer"
    ],
    "richTextBannerDismissed": true,
    // "api.token": "",
    "spellChecker.languages": [
        "en-GB"
    ],
    "sync.target": 0,
    "theme": 2,
    "ui.layout": {
        "children": [
            {
                "key": "sideBar",
                "visible": true,
                "width": 250
            },
            {
                "key": "noteList",
                "visible": true,
                "width": 250
            },
            {
                "key": "editor",
                "visible": true
            }
        ],
        "key": "root",
        "visible": true
    }
}
